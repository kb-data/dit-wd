1543178530617:sound.tidy$time <-  gsub(",","", sound.tidy$time)
1543178530659:sound.tidy$noise.level <-  as.numeric(gsub(",","", sound.tidy$noise.level))
1543178530705:sound.tidy$date.sound <-  as.Date(sound.tidy$date.sound, format='%d/%m/%Y')
1543178530742:sound.tidy$Time <- paste(format(strptime(sound.tidy$time,format='%H:%M:%S'), "%H"), ':00', sep='')
1543178531430:sound.tidy.summary <- sound.tidy %>% group_by(date.sound, Time) %>% summarise(mean.noise.level=mean(noise.level))
1543178531518:sound.tidy.summary$Week <- paste('Week', strftime(sound.tidy.summary$date.sound, "%V"), sep='_')
1543178531638:sound.tidy.summary$day.week <- format(as.Date(sound.tidy.summary$date.sound), "%A")
