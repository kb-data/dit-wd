1543178518429:}
1543178518431:dates.df <- rbind(dates.df, date.i)
1543178518435:date.i <- NULL
1543178518437:}
1543178522888:# running through each file and loading
1543178522888:sound.all <- data.frame(matrix(ncol = 4, nrow = 0))
1543178522888:colnames(sound.all) <- c('week', 'day.week', 'Time', 'mean.noise.level')
